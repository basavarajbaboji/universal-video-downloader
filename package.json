{
  "name": "universal-video-downloader",
  "version": "1.0.0",
  "description": "A universal video downloader supporting 1000+ websites",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "build:prod": "npm run build && npm start",
    "setup": "npm install && cd client && npm install && cd .."
  },
  "dependencies": {
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "video",
    "downloader",
    "youtube",
    "universal",
    "mp3",
    "mp4"
  ],
  "author": "Universal Video Downloader Team",
  "license": "MIT"
}
